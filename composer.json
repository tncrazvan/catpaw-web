{
  "name": "catpaw/web",
  "type": "project",
  "description": "A web server for CatPaw",
  "homepage": "https://github.com/tncrazvan/catpaw-web",
  "license": "MIT",
  "keywords": [
    "php",
    "cli",
    "server",
    "http",
    "websocket",
    "catpaw"
  ],
  "scripts": {
    "load": "@composer dump-autoload -o",
    "script": "@php ./vendor/catpaw/core/scripts/start.php",
    "start": "@script src/main.php",
    "dev": "@start dev 100 && @composer run dev"
  },
  "authors": [
    {
      "name": "Razvan Tanase",
      "email": "tangent.jotey@gmail.com",
      "homepage": "https://github.com/tncrazvan",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/lib/"
    }
  },
  "require": {
    "php": "^8.0",
    "catpaw/core": "^1.0",
    "amphp/amp": "^2.6",
    "amphp/file": "^2.0",
    "erusev/parsedown": "^1.7",
    "ext-pcntl": "*",
    "ext-mbstring": "*"
  },
  "config": {
    "preferred-install": "dist",
    "process-timeout": 0
  }
}
