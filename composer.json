{
  "name": "catpaw/web",
  "type": "project",
  "description": "A web server for CatPaw",
  "homepage": "https://github.com/tncrazvan/catpaw-web",
  "license": "MIT",
  "keywords": [
    "php",
    "cli",
    "server",
    "http",
    "websocket",
    "catpaw"
  ],
  "scripts": {
    "load": "@composer dump-autoload -o",
    "script": "@php ./vendor/catpaw/core/scripts/start.php",
    "start:example:cats": "@script examples/cats/main.php",
    "dev:example:cats": "@script examples/cats/main.php dev 100 && @composer run dev",
    "start:example:params": "@script examples/params/main.php",
    "dev:example:params": "@script examples/params/main.php dev 100 && @composer run dev",
    "start:example:not-found": "@script examples/not-found/main.php",
    "dev:example:not-found": "@script examples/not-found/main.php dev 100 && @composer run dev",
    "start:example:session": "@script examples/session/main.php",
    "dev:example:session": "@script examples/session/main.php dev 100 && @composer run dev",
    "start:example:application-scoped-entry": "@script examples/application-scoped-entry/main.php",
    "dev:example:application-scoped-entry": "@script examples/application-scoped-entry/main.php dev 100 && @composer run dev",
    "start:example:modules": "@script examples/modules/main.php",
    "dev:example:modules": "@script examples/modules/main.php dev 100 && @composer run dev",
    "start:example:web-socket": "@script examples/web-socket/main.php",
    "dev:example:web-socket": "@script examples/web-socket/main.php dev 100 && @composer run dev",
    "start:example:byte-range": "@script examples/byte-range/main.php",
    "dev:example:byte-range": "@script examples/byte-range/main.php dev 100 && @composer run dev",
    "start:example:custom-attributes": "@script examples/custom-attributes/main.php",
    "dev:example:custom-attributes": "@script examples/custom-attributes/main.php dev 100 && @composer run dev",
    "start:example:filters": "@script examples/filters/main.php",
    "dev:example:filters": "@script examples/filters/main.php dev 100 && @composer run dev"
  },
  "authors": [
    {
      "name": "Razvan Tanase",
      "email": "tangent.jotey@gmail.com",
      "homepage": "https://github.com/tncrazvan",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "CatPaw\\Web\\": "src/lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Examples\\ApplicationScopedEntry\\": [
        "examples/application-scoped-entry/lib/"
      ],
      "Examples\\Modules\\": [
        "examples/modules/lib/"
      ]
    }
  },
  "require": {
    "php": "^8.0",
    "amphp/amp": "^2.6",
    "amphp/file": "^2.0",
    "erusev/parsedown": "^1.7",
    "ext-pcntl": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "catpaw/core": "^1.1"
  },
  "config": {
    "preferred-install": "dist",
    "process-timeout": 0
  }
}
